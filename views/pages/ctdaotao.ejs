<script>
    // Hàm này sẽ được gọi khi người dùng nhấp vào nút Xem thêm
    function loadMoreRows() {
        // Lấy tất cả các hàng bị ẩn trong bảng
        const hiddenRows = document.querySelectorAll('#courseTable tbody tr.hidden-row');

        // Hiển thị 5 hàng đầu tiên trong số các hàng bị ẩn
        for (let i = 0; i < 5; i++) {
            if (hiddenRows[i]) {
                hiddenRows[i].style.display = 'table-row';
            }
        }

        // Nếu không còn hàng bị ẩn nữa, ẩn nút Xem thêm
        if (hiddenRows.length <= 5) {
            document.getElementById('loadMoreBtn').style.display = 'none';
        }
    }
</script>
